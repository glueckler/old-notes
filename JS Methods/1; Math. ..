Math.random()
//random number between 0 (inclusive) and 1 (exclusive)

-

Math.floor
//rounds down to the nearest whole number
//ex
console.log(Math.floor(Math.random() * 10));
// â†’ 2

-

.ceil
//rounds up to the nearest whole number

-

.round( input )
//rounds to the nearest whole number






